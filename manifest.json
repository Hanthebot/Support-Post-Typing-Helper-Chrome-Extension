{
  "manifest_version": 3,
  "name": "S-post",
  "version": "1.2.2",
  "author": "hanthebot",
  "action": {
    "default_title": "Tap to paste",
    "default_popup": "popup.html"
    },
  "icons":{"128": "icon.png"},
  "description": "서포트포스트 한국 주소 입력 단축키 프로그램",
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["https://www.s-post.kr/*"]
  }],
  "commands":{
    "submitNaver": {
      "suggested_key": {"default": "Ctrl+Shift+1"},
      "description": "Submit the information in the clipboard for Naver"
      },
    "submitCoupang": {
      "suggested_key": {"default": "Ctrl+Shift+2"},
      "description": "Submit the information in the clipboard for Coupang"
      }
    },
  "background": {"service_worker": "./background.js"},
  "permissions":["tabs", "clipboardRead", "background"]
}